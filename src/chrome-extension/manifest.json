{
  "manifest_version": 2,
  "name": "By Weekly",
  "version": "1.0.0",
  "description": "Opens By Weekly in a new tab.",
  "permissions": ["tabs"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
